{
    "name": "Canvas Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["scripting", "tabs", "activeTab"],
    "action": {
      "default_popup": "html/popup.html",
      "default_icon": "pics/canvas_logo.png"
    },
    "content_scripts": [
      { 
        "matches": ["https://*/*"],
        "js": ["js/content.js"]
      }
    ]
  }